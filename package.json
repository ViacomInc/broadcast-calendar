{
  "name": "@viacomcbs/broadcast-calendar",
  "version": "4.0.2",
  "description": "Broadcast Calendar Dates Functions",
  "author": "ViacomCBS",
  "license": "Apache-2.0",
  "bin": {
    "broadcast-calendar": "./bin/broadcast-calendar.js"
  },
  "main": "./build/index.js",
  "module": "./build/index.es.js",
  "types": "./build/lib/index.d.ts",
  "scripts": {
    "bin": "node ./bin/broadcast-calendar.js",
    "prebuild": "rimraf ./build",
    "build": "rollup --config rollup.config.ts --configPlugin @rollup/plugin-typescript",
    "lint": "eslint .",
    "precommit": "npm run lint",
    "release": "npm run test && npm run lint && npm run build && release-it",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ViacomInc/broadcast-calendar.git"
  },
  "keywords": [
    "date",
    "dates",
    "broadcast",
    "calendar"
  ],
  "peerDependencies": {
    "luxon": "^3.0.0"
  },
  "private": false,
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@rollup/plugin-commonjs": "^28.0.0",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-typescript": "^12.0.0",
    "@types/eslint__js": "^8.42.3",
    "@types/luxon": "^3.3.0",
    "@types/node": "^22.0.0",
    "@vitest/coverage-v8": "^3.0.7",
    "eslint": "^9.0.0",
    "eslint-plugin-functional": "^9.0.1",
    "prettier": "^3.0.0",
    "release-it": "^19.0.0",
    "rimraf": "^6.0.0",
    "rollup": "^4.0.0",
    "rollup-plugin-node-externals": "^8.0.0",
    "type-fest": "^4.8.3",
    "typescript": "^5.1.3",
    "typescript-eslint": "^8.2.0",
    "vitest": "^3.2.4"
  }
}
